{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "HelloWorldFunction",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/.aws-sam/build/HelloWorldFunction",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "outFiles": [
        "${workspaceRoot}/.aws-sam/build/HelloWorldFunction/app.js"
      ],
      "sourceMaps": true
    },
    {
      "name": "StaticContent",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/.aws-sam/build/StaticContent",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "outFiles": [
        "${workspaceRoot}/.aws-sam/build/StaticContent/app.js"
      ],
      "sourceMaps": true
    }
  ]
}